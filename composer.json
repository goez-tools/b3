{
  "name": "goez/b3",
  "description": "A skeleton for building composer based library",
  "authors": [
    {
      "name": "Your Name",
      "email": "yourname@example.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0",
    "mockery/mockery": "^1.0",
    "spatie/phpunit-watcher": "^1.3",
    "codedungeon/phpunit-result-printer": "^0.5"
  },
  "autoload": {
    "psr-4": {
      "Ns\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "watch": "vendor/bin/phpunit-watcher watch"
  },
  "license": "MIT"
}
